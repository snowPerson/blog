
{{#section 'link'}}
<style type="text/css">
	form{
		margin: 50px 0;
	}
</style>
{{/section}}

{{> nav}}
<div class="container">
	<div class="row">
		<h3 class="alert alert-info" role="alert">DEMO 上传</h3>
	</div>
</div>

<div class="container">
	<div class="row">
		<form role="form" class="form-horizontal col-md-10">
			<div class="form-group">
				<label for="title" class="col-md-2 control-label">标题：</label>
				<div class="col-md-10">
					<input type="text" class="form-control" id="title" placeholder="请输入DEMO标题">
				</div>
			</div>
			<br>
			<div class="form-group">
				<label for="desc" class="col-md-2 control-label">描述：</label>
				<div class="col-md-10">
					<input type="text" class="form-control" id="desc" placeholder="请输入DEMO描述信息">
				</div>
			</div>
			<br>
			<div class="form-group">
				<label for="height" class="col-md-2 control-label">高度：</label>
				<div class="col-md-10">
					<input type="text" class="form-control" id="height" placeholder="请输入图片高度">
				</div>
			</div>
			<br>
			<div class="form-group">
				<label for="file" class="col-md-2 control-label">文件：</label>
				<div class="col-md-10">
					<input type="file" class="form-control" id="file" placeholder="请选择DEMO文件">
				</div>
			</div>
			<br>
			<div class="form-group">
				<div class="col-md-8 col-md-offset-2">
					<button type="button" class="btn btn-primary" id="send">提交</button>
				</div>
			</div>
		</form>
	</div>
</div>

{{#section 'script'}}
<script type="text/javascript" src="/js/admin/demo.js"></script>
{{/section}}